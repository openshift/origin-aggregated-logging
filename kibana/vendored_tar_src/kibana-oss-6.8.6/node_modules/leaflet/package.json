{
  "name": "leaflet",
  "version": "1.0.3",
  "description": "JavaScript library for mobile-friendly interactive maps",
  "devDependencies": {},
  "main": "dist/leaflet-src.js",
  "style": "dist/leaflet.css",
  "scripts": {
    "test": "jake test",
    "build": "jake build",
    "release": "./build/publish.sh"
  },
  "eslintConfig": {
    "root": true,
    "globals": {
      "L": true
    },
    "env": {
      "commonjs": true,
      "amd": true,
      "node": false
    },
    "extends": "mourner",
    "rules": {
      "no-mixed-spaces-and-tabs": [
        2,
        "smart-tabs"
      ],
      "indent": [
        2,
        "tab",
        {
          "VariableDeclarator": 0
        }
      ],
      "curly": 2,
      "spaced-comment": 2,
      "strict": 0,
      "wrap-iife": 0,
      "key-spacing": 0,
      "consistent-return": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Leaflet/Leaflet.git"
  },
  "keywords": [
    "gis",
    "map"
  ],
  "license": "BSD-2-Clause",
  "cleaned": true
}