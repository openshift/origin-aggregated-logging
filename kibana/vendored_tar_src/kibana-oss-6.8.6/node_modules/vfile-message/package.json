{
  "name": "vfile-message",
  "version": "1.0.0",
  "description": "Create a virtual message",
  "license": "MIT",
  "keywords": [
    "vfile",
    "virtual",
    "message"
  ],
  "repository": "vfile/vfile-message",
  "bugs": "https://github.com/vfile/vfile-message/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {},
  "devDependencies": {},
  "scripts": {
    "build-md": "remark . -qfo",
    "build-bundle": "browserify index.js --bare -s vfileMessage > vfile-message.js",
    "build-mangle": "esmangle vfile-message.js > vfile-message.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "esnext": false,
    "ignores": [
      "vfile-message.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  },
  "cleaned": true
}